{
  "name": "agentpassvault-frontend",
  "private": true,
  "license": "GPL-3.0",
  "scripts": {
    "gen-dtos": "openapi-generator-cli generate -i ../docs/openapi.yaml -g typescript-fetch -o ./packages/sdk/src/api/generated --additional-properties=typescriptThreePlus=true,modelPropertyNaming=original",
    "sync-dtos": "../scripts/management/sync-dtos.sh",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.28.2",
    "prettier": "latest",
    "tsx": "^4.21.0",
    "turbo": "latest",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@8.15.1"
}
