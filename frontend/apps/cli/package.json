{
  "name": "@agentpassvault/cli",
  "version": "0.1.0",
  "private": true,
  "license": "GPL-3.0",
  "type": "module",
  "bin": {
    "agentpassvault": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist",
    "dev": "tsc -w",
    "start": "node ./dist/index.js",
    "pretest:integration": "npm run build",
    "test": "vitest run --passWithNoTests",
    "test:unit": "vitest run --exclude test/e2e --passWithNoTests",
    "test:integration": "vitest run test/e2e"
  },
  "dependencies": {
    "@agentpassvault/sdk": "workspace:*",
    "commander": "^12.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.19.33",
    "typescript": "^5.9.3"
  }
}
